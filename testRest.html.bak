<!DOCTYPE html>
<html>
<head> 
<meta charset="utf-8"> 
<title>GOGIS</title> 
</head>
<body style=" margin: 0;overflow: hidden;background: #fff;width: 100%;height:100%">

<div id="map">
	
</div>
	
<script type="text/javascript">
	var url = "http://127.0.0.1:8088/"
	var mapname = "JBNTBHTB"
	var row = 6
	var col = 8
	var size = 256
	var ratio = 2

	var map=document.getElementById("map")
	for(var i=0; i<row; i++) {
		var div = document.createElement("div")
		for(var j=0; j<col; j++) {
			var img = document.createElement("img")
			var src = url + mapname + "/" + size + "/" + i + "/" + j + "/" + ratio
			var style = "position:fixed;left:" + j*256 + "px;top:" + i*256 +"px;"
			var border = "0"
			img.src = src
			img.style = style
			img.border = border
			div.appendChild(img)
		}
		map.appendChild(div)
	}

</script>	

</body>
</html>