<!doctype html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/openlayers/openlayers.github.io@master/en/v6.4.3/css/ol.css" type="text/css">
    <style>
      .map {
        height: 800px;
        width: 100%;
      }
    </style>
    <script src="https://cdn.jsdelivr.net/gh/openlayers/openlayers.github.io@master/en/v6.4.3/build/ol.js"></script>
    <title>OpenLayers Cache</title>
  </head>
  <body>
    <h2>Cache Map</h2>
    <div id="map" class="map"></div>
    <script type="text/javascript">

  var proj_4326 = new ol.proj.get("EPSG:4326");
  var proj_4326Extent = proj_4326.getExtent();
  var mapWidth4326 = ol.extent.getWidth(proj_4326.getExtent());

//  var resolutions4326 = [
var resolutions4326 = [  0.703125,0.3515625,0.17578125,0.087890625,0.0439453125,
				0.02197265625,0.010986328125,0.0054931640625,0.00274658203125,0.001373291015625,
				0.0006866455078125,0.00034332275390625,0.000171661376953125,0.0000858306884765625];
				// ,0.00004291534423828125];

  /**
   * 网格标注
   * @type {ol.tilegrid.TileGrid}
   */
  var tileGrid = new ol.tilegrid.TileGrid({
    resolutions: resolutions4326,
    tileSize: [256, 256],
    extent: proj_4326Extent,
    origin: ol.extent.getTopLeft(proj_4326Extent),
  });

  var tilesource = new ol.source.TileImage({
    tileUrlFunction: function (tileCoord) {
      var z = tileCoord[0];
      var xx = tileCoord[1];
      var yy = Math.abs(tileCoord[2]);
      return "http://localhost:8088/" + z + "/" + xx + "/" + yy;
    },
    tileGrid: tileGrid,
    projection: proj_4326,
  });

  var AMap = new ol.layer.Tile({
    source: tilesource,
    projection: proj_4326,
  });

  var map = new ol.Map({
    layers: [
      AMap,
      // 加载本地瓦片
/*      new ol.layer.Tile({
        source: new ol.source.TileDebug({
          projection: proj_4326,
          tileGrid: tileGrid
        })
      })
	  */
    ],

    target: 'map',
    view: new ol.View({
//      center: ol.proj.transform([120, 30], 'EPSG:4326', 'EPSG:3857'),
      center: [110, 24],
      resulotions: resolutions4326,
      zoom: 1,
      minZoom: 0,
      maxZoom: 13
    }),
  });


    </script>
  </body>
</html>